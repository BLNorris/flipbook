<h1>Log In</h1>

<% if @user %>
Username or password incorrect

<% end %>

<%= form_tag logins_path do %>
  
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>
  
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  
  <%= submit_tag "Log In" %>
<% end %>